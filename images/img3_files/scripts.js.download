jQuery(document).ready(function($){$('ul.tabs li').click(function(){var tab_id=$(this).attr('data-tab');$('ul.tabs li').removeClass('current');$('.tab-content').removeClass('current');$(this).addClass('current');$("#"+tab_id).addClass('current')})
$('.home-where-ive-been__category-name').on('click',function(event){$(this).parent().find('ul').slideToggle('fast');var signElement=$(this).parent().find('.home-where-ive-been__category-name-sign');var signValue=$(signElement).text();if(signValue==='+'){$(signElement).text('-')}else if(signValue==='-'){$(signElement).text('+')}})})